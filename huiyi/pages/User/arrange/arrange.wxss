/* pages/Administration/arrange/arrange.wxss */
page {
	height: 100%;
}

.huiyi-box {
	margin: 0rpx 24rpx 0;
	padding-bottom: 100rpx;
}

.huiyi-box>.list {
	padding: 0 30rpx;
	margin-top: 30rpx;
	background: white;
	border-radius: 20rpx;
}

.huiyi-box .list .title {
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: 106rpx;
}

.huiyi-box .list .title .left {
	display: flex;
	align-items: center;
}

.huiyi-box .list .title .left .img-box {
	width: 36rpx;
	height: 36rpx;
	margin-right: 10rpx;
}

.huiyi-box .list .title .left .img-box image {
	width: 100%;
}

.huiyi-box .list .title .left .text1 {
	font-size: 32rpx;
	font-family: Noto Sans S Chinese;
	font-weight: bold;
	color: #333333;
	line-height: 36rpx;
}

.huiyi-box .list .title .right {
	font-size: 28rpx;
	font-family: Noto Sans S Chinese;
	font-weight: 400;
	color: #999999;
	line-height: 36rpx;
}

.huiyi-box .list .title .right .active {
	color: #04CF66;
}

.huiyi-box .list .table-header {
	background: #F2F6FC;
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: 80rpx;
	border-radius: 10rpx;
	font-size: 30rpx;
	font-family: Noto Sans S Chinese;
	font-weight: 500;
	color: #878D99;
	line-height: 36rpx;
}

.huiyi-box .list .table-header>view {
	padding-left: 30rpx;
}

.huiyi-box .list .table-header .left {
	width: 40%;
}

.huiyi-box .list .table-header .right {
	width: 60%;
}

.huiyi-box .list .table-body .list {
	/* background: #F2F6FC; */
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-radius: 10rpx;
	font-size: 30rpx;
	font-family: Noto Sans S Chinese;
	font-weight: 500;
	color: #333;
	line-height: 38rpx;
	padding: 24rpx 0;
	border-bottom: 1rpx solid #E6E6E6;
}

.huiyi-box .list .table-body .list>view {
	padding-left: 30rpx;
}

.huiyi-box .list .table-body .list .left {
	width: 40%;
}

.huiyi-box .list .table-body .list .right {
	width: 60%;
}

.huiyi-box .list .table-body .list .right>view {
	margin-bottom: 15rpx;
}

.huiyi-box .list .table-body .list .right>view:last-child {
	margin-bottom: 0;
}

.huiyi-box .list .showMore {
	font-size: 30rpx;
	font-family: Noto Sans S Chinese;
	font-weight: 400;
	color: #3377FF;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.huiyi-box .list .showMoreNone {
	font-size: 30rpx;
	font-family: Noto Sans S Chinese;
	font-weight: 400;
	color: #666;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}


.swiper-box {
  display: block;
  /* height: 100%; */
  width: 100%;
  overflow: hidden;
}

.swiper-box .list {
  margin: 24rpx 24rpx 0;
  background: white;
  border-radius: 14rpx;
  padding: 0 20rpx 0rpx;
}
.swiper-box .list .tit {
  font-size: 28rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* border-bottom: 1px solid #EDEDED; */
}
.swiper-box .list .tit .no .select{
  width: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  border-radius: 50%;
  border: 1px solid #CCCCCC;
  margin-right: 20rpx;
}
.swiper-box .list .tit .no .img-box{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.swiper-box .list .tit .no .img-box image{
  width: 100%;
}
.swiper-box .list .tit .no .text1{
  font-size: 30rpx;
  font-family: Noto Sans S Chinese;
  font-weight: bold;
  color: #333333;
  line-height: 36rpx;
}
.swiper-box .list .tit .status{
  display: flex;
  align-items: center;
  justify-content: center;
}
.swiper-box .list .tit .status .img-box{
  width: 22rpx;
  height: 34rpx;
  margin-left: 10rpx;
}
.swiper-box .list .tit .status .img-box image{
  width: 100%;
}
.swiper-box .list .tit .status > button{
  width: auto;
  height: 40rpx;
  background: rgba(51, 119, 255, 0.1);
  border: 1rpx solid #7AA6FF;
  border-radius: 8rpx;
  font-size: 24rpx;
  padding: 0 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 1;
}
.swiper-box .list .tit .status > button.dsp{
  color: #3377FF;
  border: 1rpx solid #7AA6FF;
  background: rgba(51, 119, 255, 0.1);
}
.swiper-box .list .tit .status > button.ytg{
  color: #67C23A;
  border: 1px solid #91E169;
  background: rgba(103, 194, 58, 0.1);
}
.swiper-box .list .tit .status > button.btg{
  color: #F56C6C;
  border: 1rpx solid #FD8E8E;
  background: rgba(245, 108, 108, 0.1);
}
.swiper-box .list .tit .status > button.ywc{
  color: #333333;
  border: 1rpx solid #F5F5F5;
  background: rgba(51, 119, 255, 0.1);
}
.swiper-box .list .tit .no .select_active{
  width: 40rpx;
  height: 40rpx;
  font-size: 0;
  margin-right: 20rpx;
}
.swiper-box .list .tit .no .select_active image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .tit .no{
  display: flex;
  align-items: center;
}
.swiper-box .list .tit .no .text{
  color: #666666;
}
.swiper-box .list .tit .status {
  color: #333333;
}
.swiper-box .list .address {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0;
}
.swiper-box .list .address .area {
  text-align: center;
}
.swiper-box .list .address .area .dizhi {
  font-size: 34rpx;
  font-family: NotoSansHans;
  font-weight: bold;
  color: #333333;
  margin-bottom: 15rpx;
}
.swiper-box .list .address .area .name {
  font-size: 28rpx;
  font-family: NotoSansHans;
  font-weight: 400;
  color: #999999;
}
.swiper-box .list .address .jiant {
  width: 100rpx;
  font-size: 0;
}
.swiper-box .list .address .jiant image{
  width: 100%;
  height: 26rpx;
}
.swiper-box .list .info{
  font-size: 26rpx;
  color: #999;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 30rpx;
}
.swiper-box .list .info .lis{
  /* width: 25%; */
  margin-right: 50rpx;
  display: flex;
  align-items: center;
}
.swiper-box .list .info .lis .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.swiper-box .list .info .lis .img-box image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .time{
  /* display: flex;
  align-items: center; */
  font-size: 28rpx;
  color: #999;
  padding: 10rpx 0 30rpx;
  /* margin-bottom: 40rpx; */
}
.swiper-box .list .time > view{
  margin-bottom: 20rpx;
  line-height: 30rpx;
}
.swiper-box .list .time > view image{
  width: 30rpx;
  height: 30rpx;
  /* margin-right: 10rpx; */
  margin-top: 3rpx;
  vertical-align: top;
  font-size: 0;
}
.swiper-box .list .time > view:last-child{
  margin-bottom: 0;
}
.swiper-box .list .time .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.swiper-box .list .time .img-box image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .but-box {
  text-align: right;
}
.swiper-box .list .but-box button{
  width: 160rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  padding: 0;
  line-height: 60rpx;
  font-weight: 400;
  display: inline-block;
  margin-left: 20rpx;
}
.swiper-box .list .but-box button.cacel{
  border: 2rpx solid #CCCCCC;
  background: white;
  color: #ccc;
}
.swiper-box .list .but-box button.confirm{
  background: #FFC53B;
  color: #333;
}
.swiper-box .kaipiao-box{
  padding-bottom: 0rpx;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore .image{
  width:200rpx;
  height: 200rpx!important;
  font-size: 0;
  margin-bottom: 20rpx;
}
.weui-loadmore__tips {
  display: block;
  vertical-align: middle;
  text-align: center;
}

.inp-box {
  border-top: 1px solid transparent;
  border-bottom: 1px solid transparent;
  background: white;
}

.input-box {
  background: #F5F5F5;
  border-radius: 34rpx;
  padding: 0 34rpx;
  margin: 30rpx 24rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  overflow: hidden;
}

.input-box .img-box {
  width: 30rpx;
  height: 30rpx;
  font-size: 0;
  flex-shrink: 0;
  margin-right: 10rpx;
}

.input-box .img-box image {
  width: 100%;
  height: 100%;
  object-fit: fill;
}

.input-box input {
  width: 100%;
  height: 68rpx;
  font-size: 28rpx;
  color: #333;
}
