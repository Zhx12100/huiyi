.model {
  position: fixed;
  /* top: -100%; */
  left: 0;
  right: 0;
  height: 100%;
  margin: auto auto;
  background: rgba(0, 0, 0, 0.5);
  /* display: flex;
  align-items: center; */
  transition: .5s;
  z-index: 12345;
}

.model .content {
  padding-top: 18rpx;
  padding-bottom: 40rpx;
  position: relative;
  background: #FFFFFF;
  border-radius: 20rpx;
  /* margin: 0 45rpx 0; */
  width: 100%;
  /* height: 80%; */
  overflow: hidden;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 123456;
}

.model .content .img-box {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 24rpx;
  top: 20rpx;
}

.model .content .img-box image {
  width: 100%;
  height: 100%;
}

.model .content .title {
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  font-weight: bold;
}

.model .content .cont {
  margin: 40rpx 30rpx 0;
  height: 84%;
  overflow-y: scroll;
}

.model .content .cont .list {
  margin-bottom: 40rpx;
}

.model .content .cont .list .top {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.model .content .cont .list .bottom {
  color: #333333;
  font-size: 28rpx;
}
.model .content .cont textarea{
  width: 99%;
  height: 360rpx;
  border: 2rpx solid #E6E6E6;
  border-radius: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #444;
}

.model .content .but-box {
  margin: 0;
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translateX(-50%);
  width: 90%;
  box-sizing: border-box;
}

.model .content .but-box button {
  width: 100%;
  height: 88rpx;
  background: #3377FF;
  border-radius: 6rpx;
  font-size: 34rpx;
  color: white;
}

.model .way-box{
  background: white;
  border-radius: 40rpx;
  margin: 0 0rpx 108rpx;
  padding: 0 0rpx;
  /* box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5); */
}
.model .way-box .title{
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  border-bottom: 1px solid #E6E6E6;
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  line-height: 1;
}
.model .way-box .way label{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  border-bottom: 1px solid #E6E6E6;
}
.model .way-box .way label[aria-checked="true"] .name{
  color : #3377FF;
}
.model .way-box .way label:last-child{
  border-bottom: none;
}
.model .way-box .way label .name{
  font-size: 28rpx;
  color: #333;
  display: inline-block;
}
.model .way-box .way label .tips{
  font-size: 26rpx;
  color: #666;
  display: inline-block;
}

.in-b{
