/* pages/feedBackResult/feedBackResult.wxss */
page{
  background: white;
}
.bar{
  background: #F3F5FA;
  width: 100%;
  height: 24rpx;
}
.cont {
  text-align: center;
  margin-top: 80rpx;
}
.cont .img-box{
  width: 152rpx;
  height: 152rpx;
  display: inline-block;
  margin-bottom: 30rpx;
}
.cont .img-box image{
  width: 100%;
  height: 100%!important;
  
}
.cont .text1{
  font-size: 40rpx;
  font-family: Noto Sans S Chinese;
  font-weight: 500;
  color: #333333;
  margin-bottom: 25rpx;
}
.cont .text2{
  font-size: 28rpx;
  font-family: Noto Sans S Chinese;
  font-weight: 400;
  color: #999999;
  margin-bottom: 80rpx;
}
.cont button{
  width: 600rpx!important;
  height: 74rpx;
  border-radius: 8px;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #3377FF;
  color: white;
  border: 2px solid #3377FF;
}